{
  "name": "n8n-nodes-bgg",
  "version": "0.1.13",
  "description": "n8n community node for BoardGameGeek - Search and retrieve board game information",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "boardgamegeek",
    "bgg",
    "boardgames",
    "automation"
  ],
  "license": "MIT",
  "homepage": "https://github.com/webthingee/n8n-nodes-bgg",
  "author": {
    "name": "Sean Lange",
    "email": "sean.lange@example.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webthingee/n8n-nodes-bgg.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes/**/*.ts",
    "lintfix": "eslint nodes --fix",
    "prepublishOnly": "npm run build && npm run lint",
    "test": "jest"
  },
  "files": [
    "dist",
    "index.js"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/Bgg/Bgg.node.js"
    ],
    "credentials": [
      "dist/credentials/BggApi.credentials.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/request-promise-native": "~1.0.15",
    "@typescript-eslint/parser": "^5.45.0",
    "@types/axios": "^0.14.0",
    "@types/xml2js": "^0.4.14",
    "@types/jest": "^29.5.12",
    "eslint-plugin-n8n-nodes-base": "^1.12.0",
    "gulp": "^4.0.2",
    "n8n-core": "^0.125.0",
    "n8n-workflow": "^0.107.0",
    "prettier": "^2.7.1",
    "typescript": "~4.8.4",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2"
  },
  "dependencies": {
    "n8n-core": "^0.125.0",
    "n8n-workflow": "^0.107.0",
    "axios": "^1.6.7",
    "xml2js": "^0.6.2"
  }
} 